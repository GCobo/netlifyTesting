{
  "name": "joinble-ui",
  "version": "1.54.5",
  "description": "Joinble UI is a set of React Components that implements theming of Joinble Creator",
  "author": "joinble",
  "license": "MIT",
  "repository": "joinble/joinble-ui",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "cypress run --headless",
    "test:open": "cypress open",
    "test:run": "node ./scripts/cyrun.js",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "semantic-release": "semantic-release",
    "publish:preview": "semantic-release -d"
  },
  "peerDependencies": {
    "@emotion/babel-preset-css-prop": "11.0.0",
